{
    "name": "romanzipp/laravel-model-doc",
    "description": "Laravel Model PHPDoc Generator",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "romanzipp",
            "email": "ich@ich.wtf",
            "homepage": "https://ich.wtf"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "doctrine/dbal": "^3.0",
        "gossi/docblock": "^2.0",
        "illuminate/console": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "romanzipp\\ModelDoc\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "romanzipp\\ModelDoc\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "romanzipp\\ModelDoc\\Providers\\ModelDocServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
